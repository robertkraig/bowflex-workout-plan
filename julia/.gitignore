# Julia-specific .gitignore

# Files generated by the Julia package manager
Manifest.toml

# Build artifacts for packages
deps/deps.jl
deps/build.log
deps/downloads/
deps/usr/
deps/src/

# Julia compiled files
*.ji

# Logs
*.log

# Temporary files created by Julia
*.tmp

# MacOS specific
.DS_Store

# Backup files
*~

# Julia environment files (optional)
# These may contain sensitive information and should be ignored
LocalPreferences.toml

# JuliaFormatter files
.JuliaFormatter.toml

# PlutoNotebook files
*.jl.html
*.jl.pdf

# Development files
test/
examples/

# Documentation build
docs/build/

# Julia history
.julia_history

# Compilation cache
*.o
*.so
